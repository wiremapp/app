{
  "name": "wiremap",
  "description": "A powerful platform for creating and managing application sitemaps.",
  "version": "1.0.0",
  "private": true,
  "build": {
    "appId": "app.wirem.app",
    "copyright": "Copyright Â© 2023 Wiremap",
    "productName": "Wiremap",
    "icon": {
      "152": "public/images/icons/152.png",
      "180": "public/images/icons/180.png",
      "512": "public/images/icons/512.png"
    }
  },
  "files": [
    "./build/**/*",
    "./dist/**/*",
    "./node_modules/**/*",
    "./public/**/*",
    "*.js",
    "*.ts"
  ],
  "directories": {
    "buildResources": "public"
  },
  "scripts": {
    "dev": "next",
    "build": "next build && npm run generate-sitemap",
    "electron-start": "electron main.ts",
    "electron-build": "electron-packager . my-electron-app --platform=darwin --arch=x64 --out=dist",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "generate-sitemap": "next-sitemap",
    "type-check": "tsc"
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.1.3",
    "cookies-next": "^2.1.1",
    "electron": "^24.3.0",
    "gray-matter": "^4.0.3",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-localstorage-backend": "^4.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.3.0",
    "next": "latest",
    "next-auth": "^4.22.1",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.0.8",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.2",
    "react-icons": "^4.8.0",
    "react-remark": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/next": "^9.0.0",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "autoprefixer": "^10.4.14",
    "electron-packager": "^17.1.1",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "webpack": "^5"
  }
}
